{
    "name": "ptflp/weeframework",
    "description": "Wee framework - slim framework for self developing",
    "keywords": [
        "wee",
        "framework"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Petr Filippov",
            "email": "globallinkliberty@yandex.ru",
            "role": "Founder and project lead"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "replace": {
        "yiisoft/yii2": "self.version"
    },
    "require": {
        "php": ">=5.4.0",
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.34",
        "squizlabs/php_codesniffer": "^3"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "wee\\": "framework/",
            "wee\\Models\\": "Models/",
            "wee\\Views\\": "Views/",
            "wee\\Controllers\\": "Controllers/"
        }
    },
    "config": {
        "platform": {"php": "5.4"}
    }
}